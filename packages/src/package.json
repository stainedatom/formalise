{
  "name": "@panhaboth/formalise",
  "version": "2.0",
  "description": "A batteries-included paginated form builder for React and Next.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stainedatom/formalise.git"
  },
  "keywords": [
    "forms",
    "react forms",
    "user input"
  ],
  "author": "Panhaboth Kun",
  "bugs": {
    "url": "https://github.com/stainedatom/formalise/issues"
  },
  "homepage": "https://formalise.panhaboth.com",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "generate:component": "turbo gen react-component",
    "build": "tsup index.tsx --format esm,cjs --dts --external react"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "react": "^18.2.0",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/lodash": "^4.14.199",
    "lodash": "^4.17.21",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "zod": "^3.22.4"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  }
